      }
    },
    {
      "cell_type": "code",
      "source": [
        "from langchain_upstage import ChatUpstage\n",
        "from langchain_core.messages import HumanMessage\n",
        "import pprint\n",
        "\n",
        "chat = ChatUpstage(api_key=\"발급받은 API 키를 입력해주세요", model=\"solar-pro3\")\n",
        "\n",
        "messages = [\n",
